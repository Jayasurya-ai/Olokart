<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:padding="5dp"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:elevation="10dp"
        app:cardElevation="10dp"
        android:padding="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="40dp"
                app:cardElevation="2dp"
                android:elevation="2dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/allcat"
            android:text="All"
            android:drawableStart="@drawable/ic_all"
            android:drawablePadding="10dp"
            android:textSize="20sp"
            android:textAllCaps="true"
            android:textColor="#42a626"
            android:fontFamily="@font/montserrat_bold"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="40dp"
                app:cardElevation="2dp"
                android:elevation="2dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/ricecat"
            android:text="Rice ,Grains &amp; Flours"
            android:textSize="20sp"
            android:drawablePadding="10dp"
            android:textAllCaps="false"
            android:drawableStart="@drawable/catrice"
            android:textColor="#42a626"
            android:fontFamily="@font/montserrat_bold"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="40dp"
                app:cardElevation="2dp"
                android:elevation="2dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/dalscat"
            android:text="Dals and Pulses"
            android:drawableStart="@drawable/dals"
            android:drawablePadding="10dp"
            android:textSize="20sp"
            android:textAllCaps="false"
            android:textColor="#42a626"
            android:fontFamily="@font/montserrat_bold"/>


            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="40dp"
                app:cardElevation="2dp"
                android:elevation="2dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/edibleoilcat"
                    android:text="Edible Oils "
                    android:drawableStart="@drawable/bottle"
                    android:textSize="20sp"
                    android:drawablePadding="10dp"
                    android:textAllCaps="false"
                    android:textColor="#42a626"
                    android:fontFamily="@font/montserrat_bold"/>


            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="40dp"
                app:cardElevation="2dp"
                android:elevation="2dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/householdcat"
                    android:text="House Hold Items "
                    android:textSize="20sp"
                    android:drawableStart="@drawable/household"
                    android:textAllCaps="false"
                    android:drawablePadding="10dp"
                    android:textColor="#42a626"
                    android:fontFamily="@font/montserrat_bold"/>


            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="40dp"
                app:cardElevation="2dp"
                android:elevation="2dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/masalalcat"
                    android:text="Masalas &amp; Spices"
                    android:drawableStart="@drawable/spices"
                    android:drawablePadding="10dp"
                    android:textSize="20sp"
                    android:textAllCaps="false"
                    android:textColor="#42a626"
                    android:fontFamily="@font/montserrat_bold" />


            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="40dp"
                app:cardElevation="2dp"
                android:elevation="2dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/perscarecat"
                    android:text="Personal Care"
                    android:drawablePadding="10dp"
                    android:drawableStart="@drawable/perfume"
                    android:textSize="20sp"
                    android:textAllCaps="false"
                    android:textColor="#42a626"
                    android:fontFamily="@font/montserrat_bold" />


            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="40dp"
                app:cardElevation="2dp"
                android:elevation="2dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/sugarsaltcat"
                    android:text="Sugars &amp; Salts"
                    android:drawableStart="@drawable/sugar"
                    android:drawablePadding="10dp"
                    android:textSize="20sp"
                    android:textAllCaps="false"
                    android:textColor="#42a626"
                    android:fontFamily="@font/montserrat_bold" />

            </androidx.cardview.widget.CardView>
        </LinearLayout>

    </androidx.cardview.widget.CardView>
</FrameLayout>
